{
    "name": "widget-screenshot",
    "version": "0.1.0",
    "description": "",
    "main": "src/js/index.js",
    "scripts": {
        "live-server": "live-server --port=3003 --open=/public",
        "build:prod": "webpack --config webpack.config.prod.js",
        "build:dev": "webpack --config webpack.config.dev.js",
        "build": "webpack --config webpack.config.js",
        "watch:prod": "webpack -w --config webpack.config.prod.js",
        "watch:dev": "webpack -w --config webpack.config.dev.js",
        "watch": "webpack -w --config webpack.config.js",
        "serve:prod": "webpack serve --config webpack.config.prod.js --hot --inline --port 3002",
        "serve:dev": "webpack serve --config webpack.config.dev.js --hot --inline --port 3002",
        "serve": "webpack serve --config webpack.config.js --hot --inline --port 3002",
        "start": "npm run watch | npm run serve",
        "test": "",
        "prettier:check": "prettier --check 'src/**/*.js'",
        "prettier": "prettier --write 'src/**/*.js'",
        "prettier:commit": "pretty-quick --staged --print-width 120",
        "lint": "eslint ./src",
        "deploy": "npm run lint && npm run build:prod && npm run sync",
        "sync": "aws s3 sync ./public s3://files-screenshotter/screenshot --profile screenshotter --endpoint-url https://fra1.digitaloceanspaces.com --region fra1 --acl public-read"
    },
    "homepage": "./",
    "author": "Jo du Plessis",
    "license": "ISC",
    "repository": "",
    "url": "",
