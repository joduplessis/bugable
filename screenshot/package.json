        "babel-loader": "^8.2.2",
        "babel-plugin-syntax-async-functions": "^6.13.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^6.4.0",
        "css-loader": "^5.0.1",
        "dotenv-webpack": "^6.0.0",
        "eslint": "^7.15.0",
